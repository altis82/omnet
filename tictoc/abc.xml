<?xml version="1.0" encoding="ISO-8859-1"?>
<network name="Tictoc2" type="Tictoc2">
    <parameters>
        <property name="isNetwork" value=""/>
    </parameters>
    <submodules>
        <module name="tic" type="Txc2">
            <parameters>
                <property name="display" value="&quot;i=block/routing,cyan&quot;"/>
            </parameters>
            <gates>
                <gate name="in" type="input"/>
                <gate name="out" type="output"/>
            </gates>
            <connections/>
        </module>
        <module name="toc" type="Txc2">
            <parameters>
                <property name="display" value="&quot;i=block/routing,gold&quot;"/>
            </parameters>
            <gates>
                <gate name="in" type="input"/>
                <gate name="out" type="output"/>
            </gates>
            <connections/>
        </module>
    </submodules>
    <connections>
        <connection src-module="tic" src-gate="out" dest-module="toc" dest-gate="in">
            <channel name="channel" type="ned.DelayChannel">
                <parameters>
                    <property name="class" value="cDelayChannel"/>
                    <property name="signal[messageSent]" value="type=omnetpp::cMessage"/>
                    <property name="signal[messageDiscarded]" value="type=omnetpp::cMessage"/>
                    <property name="statistic[messages]" value="source=constant1(messageSent);record=count?;interpolationmode=none"/>
                    <property name="statistic[messagesDiscarded]" value="source=constant1(messageDiscarded);record=count?;interpolationmode=none"/>
                    <param name="disabled" type="bool" value="false"/>
                    <param name="delay" type="double" value="0.1s">
                        <property name="unit" value="s"/>
                    </param>
                </parameters>
            </channel>
        </connection>
        <connection src-module="toc" src-gate="out" dest-module="tic" dest-gate="in">
            <channel name="channel" type="ned.DelayChannel">
                <parameters>
                    <property name="class" value="cDelayChannel"/>
                    <property name="signal[messageSent]" value="type=omnetpp::cMessage"/>
                    <property name="signal[messageDiscarded]" value="type=omnetpp::cMessage"/>
                    <property name="statistic[messages]" value="source=constant1(messageSent);record=count?;interpolationmode=none"/>
                    <property name="statistic[messagesDiscarded]" value="source=constant1(messageDiscarded);record=count?;interpolationmode=none"/>
                    <param name="disabled" type="bool" value="false"/>
                    <param name="delay" type="double" value="0.1s">
                        <property name="unit" value="s"/>
                    </param>
                </parameters>
            </channel>
        </connection>
    </connections>
</network>
